{
  "name": "funnycode",
  "displayName": "FunnyCode",
  "description": "Adds a little decoration above the written text.",
  "publisher": "ExBrain",
  "icon": "images/icon.jpg",
  "version": "1.0.2",
  "repository": {
    "url": "https://github.com/exbraiin/FunnyCode"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "funnycode.toggleEnable",
        "title": "Toggle Funny Code"
      }
    ],
    "configuration": [
      {
        "title": "General",
        "properties": {
          "funnycode.enabled": {
            "order": 0,
            "type": "boolean",
            "default": true,
            "description": "Enables funny code!"
          },
          "funnycode.cursor": {
            "order": 1,
            "type": "boolean",
            "default": false,
            "description": "Enables funny code \"cursor\" animation!"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  }
}